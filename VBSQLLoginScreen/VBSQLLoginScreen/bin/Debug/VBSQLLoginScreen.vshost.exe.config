<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client" />
    </startup>

  <appSettings>
    <add key="Server" value="ServerName"/>  <!--Change this with your Server Name-->
    <add key="Database" value="VBSQL_LoginScreenDB"/> <!--No need to edit this one-->
    <add key="Username" value="Username"/>  <!--Change this with your Server Username-->
    <add key="Password" value="EncryptedPassword"/> <!--Use this function "GetEncryptedData" in the AppConfiguration class to get your encrypted password-->
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
  </appSettings>
</configuration>